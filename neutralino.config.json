{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/react-src/dist",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": ["app.*", "filesystem.*", "os.*", "computer.*"],
  "modes": {
    "window": {
      "title": "duckdesk",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/react-src/public/icon.png"
    }
  },
  "cli": {
    "binaryName": "duckdesk",
    "resourcesPath": "/react-src/dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/public/neutralino.js",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2",
    "frontendLibrary": {
      "patchFile": "/react-src/index.html",
      "devUrl": "http://localhost:5173"
    }
  }
}
